{
  "name": "duoyun-ui",
  "version": "0.0.21",
  "description": "WebComponts UI",
  "exports": {
    "./elements/*": "./elements/*.js",
    "./lib/*": "./lib/*.js",
    "./locales/*": "./locales/*.js"
  },
  "files": [
    "/elements/",
    "/lib/",
    "/locales/"
  ],
  "scripts": {
    "docs": "node scripts/hack-gbp-example && gem-book docs --plugin node_modules/gbp-example-hack",
    "build:docs": "gem-book docs --build --plugin example",
    "clean": "node -e \"fs.readdirSync('src').map(dir => require('rimraf').sync(dir))\"",
    "build": "yarn clean && tsc -p tsconfig.build.json",
    "start": "yarn build --watch",
    "test": "wtr",
    "prepublishOnly": "yarn build"
  },
  "dependencies": {
    "d3-geo": "^3.0.1"
  },
  "peerDependencies": {
    "@mantou/gem": "^1.6.9"
  },
  "devDependencies": {
    "@esm-bundle/chai": "^4.3.4-fix.0",
    "@gemjs/config": "^1.6.10",
    "@mantou/gem": "^1.6.14",
    "@open-wc/testing": "^2.5.33",
    "@types/d3-geo": "^3.0.1",
    "@web/dev-server-esbuild": "^0.2.16",
    "@web/test-runner": "^0.13.22",
    "elkjs": "^0.7.3-dev",
    "rimraf": "^3.0.2",
    "typescript": "^4.5.0",
    "webpack-merge": "^5.7.3"
  },
  "author": "mantou132",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mantou132/gem.git",
    "directory": "packages/duoyun-ui"
  },
  "bugs": {
    "url": "https://github.com/mantou132/gem/issues"
  },
  "homepage": "https://github.com/mantou132/gem#readme"
}
